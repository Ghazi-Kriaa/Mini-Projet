@model IEnumerable<miniProjet.Models.Entreprise>

@{
    ViewData["Title"] = "Index";
}

<h1><img src="data:image/png;base64,"/></h1>

<p>
    <a asp-action="Create"><img src="data:image/png;base64,"/></a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EntrepriseName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EntrepriseAdress)
            </th>
            <th> 
                Opération
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EntrepriseName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EntrepriseAdress)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EntrepriseID"><img src="data:image/png;base64,"/></a> |
                <a asp-action="Details" asp-route-id="@item.EntrepriseID"><img src="data:image/png;base64,"/></a> |
                <a asp-action="Delete" asp-route-id="@item.EntrepriseID"><img src="data:image/png;base64,"/></a>
            </td>
        </tr>
}
    </tbody>
</table>
